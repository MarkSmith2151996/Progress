{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./main",
    "rootDir": "./main",
    "resolveJsonModule": true
  },
  "include": ["main/**/*.ts"],
  "exclude": ["node_modules"]
}
